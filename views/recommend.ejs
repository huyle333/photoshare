<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='/stylesheets/bootstrap.min.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <div class="container">
      <div class="row">
        <div class="col-md-6">
          <h3> You may also like </h3>
          <% pics.forEach(function(el, i) {%>
              <a href="/pic/<%=pics[i].photo%>"><li><%=pics[i].photo%></li></a>
          <% }) %>
        </div>
        <div class="col-md-6">
          <h3> Tag recommendation </h3>
          <% tags.forEach(function(el, i) {%>
              <li><%=tags[i].text%></li>
              <li><%=tags[i].total%></li>
          <% }) %>
        </div>
      </div>
    </div>
  </body>
</html>
